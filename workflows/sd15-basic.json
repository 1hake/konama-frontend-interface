{
  "metadata": {
    "id": "sd15-basic",
    "name": "Stable Diffusion 1.5 Basic",
    "description": "Classic Stable Diffusion 1.5 workflow with KSampler",
    "category": "stable-diffusion",
    "author": "ComfyUI",
    "version": "1.0.0",
    "tags": [
      "sd1.5",
      "basic",
      "fast"
    ],
    "supportsNegativePrompt": true,
    "requiredModels": [
      "SD1.5/v1-5-pruned-emaonly.ckpt"
    ],
    "parameters": [
      {
        "name": "steps",
        "label": "Steps",
        "type": "slider",
        "defaultValue": 20,
        "min": 1,
        "max": 150,
        "step": 1,
        "description": "Number of denoising steps"
      },
      {
        "name": "cfg",
        "label": "CFG Scale",
        "type": "slider",
        "defaultValue": 8,
        "min": 1,
        "max": 20,
        "step": 0.5,
        "description": "Classifier Free Guidance scale"
      },
      {
        "name": "width",
        "label": "Width",
        "type": "slider",
        "defaultValue": 512,
        "min": 256,
        "max": 1024,
        "step": 64,
        "description": "Image width"
      },
      {
        "name": "height",
        "label": "Height",
        "type": "slider",
        "defaultValue": 512,
        "min": 256,
        "max": 1024,
        "step": 64,
        "description": "Image height"
      },
      {
        "name": "sampler",
        "label": "Sampler",
        "type": "select",
        "defaultValue": "euler",
        "options": [
          "euler",
          "euler_ancestral",
          "heun",
          "dpm_2",
          "dpm_2_ancestral",
          "lms",
          "dpm_fast",
          "dpm_adaptive",
          "dpmpp_2s_ancestral",
          "dpmpp_sde",
          "dpmpp_2m",
          "ddim",
          "uni_pc"
        ],
        "description": "Sampling method"
      },
      {
        "name": "scheduler",
        "label": "Scheduler",
        "type": "select",
        "defaultValue": "normal",
        "options": [
          "normal",
          "karras",
          "exponential",
          "simple"
        ],
        "description": "Noise scheduler"
      },
      {
        "name": "checkpoint",
        "label": "Checkpoint",
        "type": "text",
        "defaultValue": "SD1.5/v1-5-pruned-emaonly.ckpt",
        "description": "Model checkpoint filename"
      }
    ]
  },
  "workflowTemplate": "sd15-basic"
}