{
  "metadata": {
    "id": "flux-krea-dev",
    "name": "Flux Krea Dev",
    "description": "Advanced Flux model with LoRA support and dynamic resolution",
    "category": "flux",
    "author": "Krea AI",
    "version": "1.0.0",
    "tags": [
      "flux",
      "lora",
      "high-quality",
      "flexible"
    ],
    "supportsNegativePrompt": true,
    "requiredModels": [
      "flux1-dev.sft",
      "clip_l.safetensors",
      "t5xxl_fp16.safetensors",
      "ae.safetensors"
    ],
    "parameters": [
      {
        "name": "steps",
        "label": "Steps",
        "type": "slider",
        "defaultValue": 20,
        "min": 1,
        "max": 50,
        "step": 1,
        "description": "Number of denoising steps"
      },
      {
        "name": "aspectRatio",
        "label": "Aspect Ratio",
        "type": "select",
        "defaultValue": "1:1 (Square)",
        "options": [
          "1:1 (Square)",
          "16:9 (Landscape)",
          "9:16 (Portrait)",
          "4:3 (Standard)",
          "3:4 (Portrait)"
        ],
        "description": "Image aspect ratio"
      },
      {
        "name": "guidance",
        "label": "Guidance Scale",
        "type": "slider",
        "defaultValue": 3.5,
        "min": 1,
        "max": 10,
        "step": 0.5,
        "description": "How closely to follow the prompt"
      },
      {
        "name": "loraName",
        "label": "LoRA Model",
        "type": "text",
        "defaultValue": "CynthiaArch.safetensors",
        "description": "LoRA model filename"
      },
      {
        "name": "loraStrength",
        "label": "LoRA Strength",
        "type": "slider",
        "defaultValue": 1.0,
        "min": 0,
        "max": 2,
        "step": 0.1,
        "description": "LoRA model strength"
      }
    ]
  },
  "workflowTemplate": "flux-krea-dev"
}