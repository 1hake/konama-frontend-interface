{
  "name": "boilerplate-next-tailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "build:safe": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "npx serve@latest out",
    "start:node": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests configured\" && exit 0",
    "docker:build": "docker build -t image-generation-admin .",
    "docker:build-dev": "docker build --target development -t image-generation-admin:dev .",
    "docker:run": "docker run -p 3000:80 image-generation-admin",
    "docker:run-dev": "docker run -p 3001:3000 -v $(pwd):/app image-generation-admin:dev",
    "deploy:staging": "echo 'Deploying to staging...'",
    "deploy:production": "echo 'Deploying to production...'"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@headlessui/react": "^2.2.9",
    "@types/sqlite3": "^3.1.11",
    "next": "^15.5.2",
    "openai": "^6.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-toastify": "^11.0.5",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.8.31",
    "eslint": "^8",
    "eslint-config-next": "14.2.32",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}